{
  "compilerOptions": {
    "outDir": "./dist/",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "target": "ESNext",
    "strict": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "baseUrl": "."
  },
  "exclude": [
    "node_modules",
    "dev",
    "packages"
  ]
}
